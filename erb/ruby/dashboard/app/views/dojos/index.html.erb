<h1>Listing Dojos</h1>


<table>
    <thead>
        <tr>
            <td>Branch</td>
            <td>Street</td>
            <td>City</td>
            <td>State</td>
            <td>Actions</td>
            <td></td>
            <td></td>
        </tr>
    </thead>
    <% @dojos.each do |dojo| %>
    <tbody>
        <tr>
            <td><%= dojo.branch %></td>
            <td><%= dojo.street %></td>
            <td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td><a href='dojos/<%= dojo.id %>'>Show!</a></td>
            <td><a href='dojos/<%= dojo.id %>/edit'>Edit!</a></td>
            <td><a href='dojos/<%= dojo.id %>' data-method = "delete">Destroy!</a></td>
        </tr>
    </tbody>
    <% end %>
</table>

<a href = "dojos/new">New Dojo</a>
            